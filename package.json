{
  "name": "vsjmcc",
  "publisher": "rqbik",
  "displayName": "vscode-jmcc",
  "version": "1.5.0",
  "description": "VSCode support for JustCode",
  "author": "rqbik",
  "license": "MIT",
  "keywords": [
    "JustCode"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "yarn build && yarn start",
    "package": "yarn start && vsce package"
  },
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.41"
  },
  "contributes": {
    "languages": [
      {
        "id": "justcode",
        "aliases": [
          "JustCode",
          "justcode"
        ],
        "extensions": [
          ".jc"
        ],
        "firstLine": "^import (\"|')[/\\w\\-. ]+(\"|')",
        "configuration": "./jc.language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "justcode",
        "scopeName": "source.jc",
        "path": "./JustCode.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "justcode",
        "path": "./snippets.json"
      }
    ]
  }
}
